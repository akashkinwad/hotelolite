    .container
      %h2.color-white AGRICULTURE DEVELOPMENT TRUST
/ GALLERY-DETAIL
.main-wraper.padd-70-70
  .container
    .detail-header
      .row
        .col-xs-12.col-sm-8
          .detail-category.color-grey-3 Welcome to
          %h2.detail-title.color-dark-2 AGRICULTURE DEVELOPMENT TRUST
