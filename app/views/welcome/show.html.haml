              %span.fa.fa-star.color-yellow
              %span.fa.fa-star.color-yellow
              %span.fa.fa-star.color-yellow
              %span.fa.fa-star.color-yellow
              %span.fa.fa-star.color-yellow
            %i 485 Rewies
        .col-xs-12.col-sm-4
          .detail-price.color-dark-2
            price from
            %span.color-dr-blue $200
            \/person
    .gallery-detail
      .top-baner.arrows
        #tour-slide-2.swiper-container{"data-autoplay" => "0", "data-center" => "0", "data-loop" => "1", "data-slides-per-view" => "1", "data-speed" => "1000"}
          .swiper-wrapper
            .swiper-slide
              %img.img-responsive{:alt => "", :src => "/assets/user/img/gallery/slide_1.jpg"}/
